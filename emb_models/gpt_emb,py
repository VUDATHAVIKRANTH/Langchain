from langchain_openai import OpenAIEmbeddings
from dotenv import load_dotenv

load_dotenv()

embedding = OpenAIEmbeddings(model='text-embedding-3-large', dimensions=32) #dimensions= what output dimension that I want

documents =["Delhi is the capital of India",
            "Washington DC is the Capital of USA",
            "London is the capital of UK"]
result = embedding.embed_query("what is the capital of india")

#result = embedding.embed_documents(documents)

print(result)